<html lang="en">
<head>
  	<% include  ../views/head %>
</head>
<body>
	<% include ../views/nav %>
	<div class="eleven wide column">
			<div class="section">
				<h2>FoodGallery</h2>
				<ul style="padding-left:200px;">
					<li><img src="/image/dishes-eg/american.jpg" alt="photo" height="300" width="300"></li>
					<li><img src="/image/dishes-eg/chinese.jpg" alt="photo" height="300" width="300"></li>
					<li><img src="/image/dishes-eg/japanese.jpg" alt="photo" height="300" width="300"></li>
					<% foods.forEach(function(food) {%>
				  	<li><img src="<%= food.imgpath %>" alt="photo" height="300" width="300"></li>
				  	<li><%= food.name %></li>
				  	<li><%= food.type %></li
				  	<li><%= food.description %></li>
				  	<li><%= food.create_time %></li>
				  	<% }) %>
				</ul>
			</div>
		</div>
</body>